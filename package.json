{
  "name": "firebase_react_template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "craco build",
    "eject": "craco eject",
    "format": "yarn run lint --fix & yarn prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx src",
    "start": "craco start",
    "emulators:start": "firebase emulators:start --import=firebase-data --export-on-exit=firebase-data",
    "emulators:sandbox": "firebase emulators:start --import=firebase-data",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@craco/craco": "7.1.0",
    "@reduxjs/toolkit": "^1.8.6",
    "clsx": "^2.0.0",
    "firebase": "^10.4.0",
    "queue-typescript": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-redux": "^8.0.4",
    "react-router-dom": "6.15.0",
    "sass": "1.66.1",
    "web-vitals": "^3.0.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@redux-devtools/core": "^3.13.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-html-reporter": "^3.3.0",
    "jest-junit": "^12.0.0",
    "prettier": "^3.0.3",
    "react-scripts": "5.0.1",
    "ts-jest": "29.1.1",
    "typescript": "^4.9.5"
  }
}
